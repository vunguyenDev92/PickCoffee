## PickCoffee - Local Properties Template
## Copy this file to 'local.properties' and fill in your actual API keys
## DO NOT commit 'local.properties' to git - it's already in .gitignore

# SDK Location (auto-generated by Android Studio)
sdk.dir=/path/to/your/Android/sdk

# Firebase Web Client ID
# Get this from Firebase Console -> Project Settings -> General -> Web API Key
# OR from google-services.json -> client[].oauth_client[].client_id (client_type: 3)
FIREBASE_WEB_CLIENT_ID=your-firebase-web-client-id-here

# Google AdMob IDs
# Get these from AdMob Console -> Apps -> Ad units
# For testing, you can use Google's test IDs (already in code as fallback)

# AdMob Application ID (from AdMob Console)
ADMOB_APP_ID=ca-app-pub-3940256099942544~3347511713

# AdMob Ad Unit IDs
ADMOB_APP_OPEN_AD_UNIT_ID=ca-app-pub-3940256099942544/9257395921
ADMOB_INTERSTITIAL_AD_UNIT_ID=ca-app-pub-3940256099942544/1033173712
ADMOB_REWARDED_AD_UNIT_ID=ca-app-pub-3940256099942544/5224354917
ADMOB_BANNER_AD_UNIT_ID=ca-app-pub-3940256099942544/9214589741

# Note: The IDs above are Google's test IDs
# Replace them with your production IDs from AdMob Console when ready for production
